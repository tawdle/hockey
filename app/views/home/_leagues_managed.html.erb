<% if League.managed_by(current_user).without_deleted.any? %>
  <% League.managed_by(current_user).without_deleted.each do |league| %>
    <h2 class="page-header"><%= link_to league.name, league %> <small>You manage this league</small></h2>
    <%= render :partial => "games", :locals => {:games => league.games } %>
  <% end %>
<% end %>
